        tak:
            sprawdz czy jestes na skrzyzo (teraz aktualne X, Y):
                tak:
                nie:
                    sprawdz czy moj slot jest wolny:
                        tak:
                            sprawdz czy wszystkie sloty w lewo są zajete:
                                tak:
